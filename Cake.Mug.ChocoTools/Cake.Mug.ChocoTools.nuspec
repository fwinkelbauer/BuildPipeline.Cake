<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>cake.mug.chocotools</id>
    <version>0.7.0</version>
    <title>Cake.Mug Chocolatey Tools</title>
